<table>
    <thead>
        <tr>
            <th>Patient ID</th>
            <th>Patient Name</th>
            <th>Age</th>
        </tr>
    </thead>
    <tbody>

        <% results.forEach(row = >{
            %>
            <tr>
                <td><%= row.patient_id %></td>
                <td><%= row.patient_name %></td>
                <td><%= row.age %></td>
            </tr>
            <%
        })  %>
    </tbody>
   
</table>